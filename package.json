{
  "name": "purescript-starter",
  "version": "1.0.0",
  "description": "Starter kit for purescript",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "cleanup": "shx rm -rf dist",
    "build": "spago bundle-app --main Main --to dist/esm2015/index.js --source-maps && rollup -c config/rollup.config.js && rollup -c config/rollup.config.js --environment NODE_ENV:production"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "devDependencies": {
    "rollup": "0.68.2",
    "rollup-plugin-commonjs": "9.2.0",
    "rollup-plugin-json": "3.1.0",
    "rollup-plugin-node-resolve": "4.0.0",
    "rollup-plugin-replace": "2.1.0",
    "rollup-plugin-sourcemaps": "0.4.2",
    "rollup-plugin-terser": "3.0.0",
    "rollup-plugin-uglify": "6.0.0",
    "shx": "0.3.2",
    "standard-version": "4.4.0",
    "webpack-config-utils": "2.3.1",
    "brotli-size": "0.0.3",
    "commitizen": "3.0.5",
    "cross-var": "1.1.0",
    "cz-conventional-changelog": "2.1.0",
    "gzip-size": "5.0.0",
    "husky": "1.3.0",
    "@commitlint/cli": "7.2.1",
    "@commitlint/config-conventional": "7.1.2"
  }
}
